Xft.dpi:            96
Xft.autohint:       false
Xft.lcdfilter:      lcddefault
Xft.hintstyle:      hintfull
Xft.hinting:        true
Xft.antialias:      true
Xft.rgba:           rgb

Xcursor.theme:      Neutral

*.font:             -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
*.boldFont:         -xos4-terminus-bold-r-normal--12-120-72-72-c-60-iso10646-1

*.foreground:       #D8DEE9
*.background:       #292d3e

*.cursorColor:      #D8DEE9
*.highlightColor:   #3A3F4B

! black
*.color0:           #3B4252
*.color8:           #3B4252
! red
*.color1:           #FF5370
*.color9:           #FF5370
! green
*.color2:           #A3BE8C
*.color10:          #A3BE8C
! yellow
*.color3:           #EBCB8B
*.color11:          #EBCB8B
! blue
*.color4:           #81A1C1
*.color12:          #81A1C1
! magenta
*.color5:           #B48EAD
*.color13:          #B48EAD
! cyan
*.color6:           #88C0D0
*.color14:          #88C0D0
! white
*.color7:           #E5E9F0
*.color15:          #E5E9F0
