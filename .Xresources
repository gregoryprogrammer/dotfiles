Xft.dpi: 96
Xft.autohint: false
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb

Xcursor.theme: Neutral

*.font:                 -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
*.boldFont:             -xos4-terminus-bold-r-normal--12-120-72-72-c-60-iso10646-1

! special
*.foreground:       #ABB2BF
*.background:       #171b23
*.cursorColor:      #ABB2BF
*.highlightColor:   #3A3F4B

! black
*.color0:           #171b23
*.color8:           #3E4452

! red
*.color1:           #E06C75
*.color9:           #BE5046

! green
*.color2:           #98C379
*.color10:          #98C379

! yellow
*.color3:           #E5C07B
*.color11:          #D19A66

! blue
*.color4:           #61AFEF
*.color12:          #61AFEF

! magenta
*.color5:           #C678DD
*.color13:          #C678DD

! cyan
*.color6:           #56B6C2
*.color14:          #56B6C2

! white
*.color7:           #7B8290
*.color15:          #ABB2BF
